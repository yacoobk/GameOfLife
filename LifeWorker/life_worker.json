{
  "workerType": "LifeWorker",
  "attributeSet": {
    "attributes": [
      "lifeSimulation"
    ]
  },
  "entity_interest": {
    "range_entity_interest": {
      "radius": 2
    }
  },
  "componentDelivery": {
    "default": "RELIABLE_ORDERED",
    "checkout_all_initially": true
  },
  "launchConfiguration": {
    "cloudDeployment": {
      "linux": {
        "localBuildDirectory": "bin/x64/ReleaseLinux",
        "command": "mono",
        "arguments": [
          "LifeWorker.exe",
          "${IMPROBABLE_RECEPTIONIST_HOST}",
          "${IMPROBABLE_RECEPTIONIST_PORT}",
          "${IMPROBABLE_WORKER_ID}"
        ]
      }
    },
    "localDeployment": {
      "macos": {
        "localBuildDirectory": "bin/x64/ReleaseMacOS",
        "command": "mono",
        "arguments": [
          "--arch=64",
          "LifeWorker.exe",
          "${IMPROBABLE_RECEPTIONIST_HOST}",
          "${IMPROBABLE_RECEPTIONIST_PORT}",
          "${IMPROBABLE_WORKER_ID}"
        ]
      },
      "linux": {
        "localBuildDirectory": "bin/x64/ReleaseLinux",
        "command": "mono",
        "arguments": [
          "LifeWorker.exe",
          "${IMPROBABLE_RECEPTIONIST_HOST}",
          "${IMPROBABLE_RECEPTIONIST_PORT}",
          "${IMPROBABLE_WORKER_ID}"
        ]
      },
      "windows": {
        "localBuildDirectory": "bin/x64/ReleaseWindows",
        "command": "LifeWorker.exe",
        "arguments": [
          "${IMPROBABLE_RECEPTIONIST_HOST}",
          "${IMPROBABLE_RECEPTIONIST_PORT}",
          "${IMPROBABLE_WORKER_ID}"
        ]
      }
    }
  }
}
